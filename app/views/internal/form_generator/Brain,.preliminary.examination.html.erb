<div class="panel panel-primary">
        <div class="panel-heading">
           <h4 class="panel-title">
              <a data-toggle="collapse" data-parent="#accordion" href="#Brain,-preliminary-examination">Brain,-preliminary-examination</a>
           </h4>
        </div>
      <div id="Brain,-preliminary-examination" class="panel-collapse collapse in"> 

	     <div class="row" style="width:100%">
		<div class="field col-sm-4">
		    <%= f.label :brain_weight %><br>
		<%= f.number_field :brain_weight, class: "form-control" %>
	     </div> </div>
<div class="row" style="width:100%">
		   <div class="col-sm-6 drop-down">
		      <%= f.label  :configuration_lobar_pattern %><br>
		      <%= f.select :configuration_lobar_pattern, [:Normal, :Abnormal], { :prompt => "Select One" },{ class: "form-control" , onchange: "myFunction(this,'Abnormal')"} %> 
		   </div>

		   <div class="col-sm-6">
		   <% if !@description.internal.nil?  %>
		    <% if @description.internal.configuration_lobar_pattern == "Abnormal" %>
		      <%= f.label  :please_specify, { id:"label_internal_configuration_lobar_pattern_specify" } %><br>
		      <%= f.text_field :configuration_lobar_pattern_specify, { class: "form-control", placeholder: "If Abnormal, explain" } %>
		    <% else %>
		      <%= f.label  :please_specify, { id:"label_internal_configuration_lobar_pattern_specify" } %><br>
		      <%= f.text_field :configuration_lobar_pattern_specify, { class: "form-control", placeholder: "If Abnormal, explain" , disabled: true } %>
		    <% end %>
		    <% else %>
		      <%= f.label  :please_specify, { id:"label_internal_configuration_lobar_pattern_specify" } %><br>
		      <%= f.text_field :configuration_lobar_pattern_specify, { class: "form-control", placeholder: "If Abnormal, explain" , disabled: true } %>

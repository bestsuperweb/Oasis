<div class="panel panel-primary">
        <div class="panel-heading">
           <h4 class="panel-title">
              <a data-toggle="collapse" data-parent="#accordion" href="#Heart,-chambers-and-septa">Heart,-chambers-and-septa</a>
           </h4>
        </div>
      <div id="Heart,-chambers-and-septa" class="panel-collapse collapse in"> 
<div class="row" style="width:100%">
		   <div class="col-sm-6 drop-down">
		      <%= f.label  :configuration_and_contents %><br>
		      <%= f.select :configuration_and_contents, [:Normal, :Abnormal], { :prompt => "Select One" },{ class: "form-control" , onchange: "myFunction(this,'Abnormal')"} %> 
		   </div>

		   <div class="col-sm-6">
		   <% if !@description.internal.nil?  %>
		    <% if @description.internal.configuration_and_contents == "Abnormal" %>
		      <%= f.label  :please_specify, { id:"label_internal_configuration_and_contents_specify" } %><br>
		      <%= f.text_field :configuration_and_contents_specify, { class: "form-control", placeholder: "If Abnormal, explain" } %>
		    <% else %>
		      <%= f.label  :please_specify, { id:"label_internal_configuration_and_contents_specify" } %><br>
		      <%= f.text_field :configuration_and_contents_specify, { class: "form-control", placeholder: "If Abnormal, explain" , disabled: true } %>
		    <% end %>
		    <% else %>
		      <%= f.label  :please_specify, { id:"label_internal_configuration_and_contents_specify" } %><br>
		      <%= f.text_field :configuration_and_contents_specify, { class: "form-control", placeholder: "If Abnormal, explain" , disabled: true } %>

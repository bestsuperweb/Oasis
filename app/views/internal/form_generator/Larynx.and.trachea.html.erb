           <h4 class="panel-title">
              <a data-toggle="collapse" data-parent="#accordion" href="#Larynx-and-trachea">Larynx-and-trachea</a>
           </h4>
        </div>
      <div id="Larynx-and-trachea" class="panel-collapse collapse in"> 
<div class="row" style="width:100%">
		   <div class="col-sm-6 drop-down">
		      <%= f.label  :epiglottis %><br>
		      <%= f.select :epiglottis, [:Normal, :Abnormal], { :prompt => "Select One" },{ class: "form-control" , onchange: "myFunction(this,'Abnormal')"} %> 
		   </div>

		   <div class="col-sm-6">
		   <% if !@description.internal.nil?  %>
		    <% if @description.internal.epiglottis == "Abnormal" %>
		      <%= f.label  :please_specify, { id:"label_internal_epiglottis_specify" } %><br>
		      <%= f.text_field :epiglottis_specify, { class: "form-control", placeholder: "If Abnormal, explain" } %>
		    <% else %>
		      <%= f.label  :please_specify, { id:"label_internal_epiglottis_specify" } %><br>
		      <%= f.text_field :epiglottis_specify, { class: "form-control", placeholder: "If Abnormal, explain" , disabled: true } %>
		    <% end %>
		    <% else %>

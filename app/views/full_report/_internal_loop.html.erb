<% (0...@names_array.length).each do |i| %>
    <% if @specify_array[i] == "" %>
       <%= get_view_from_array(@names_array[i], eval("@description.internal.#{@prop_array[i]}").to_s.capitalize, "") %>
    <% else %>
       <%= get_view_from_array(@names_array[i], eval("@description.internal.#{@prop_array[i]}").to_s.capitalize, eval("@description.internal.#{@specify_array[i]}") ) %>
    <% end %>
<%  end %>
